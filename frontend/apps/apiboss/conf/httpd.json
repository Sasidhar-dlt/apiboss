            "^http(.*)://([^/]+?)/index.html$": "http{{{$1}}}://{{{$2}}}/apps/apiboss/loginappframework/index.html"
        },
        {
            "^http(.*)://([^/]+?)/$": "http{{{$1}}}://{{{$2}}}/apps/apiboss/loginappframework/index.html"
